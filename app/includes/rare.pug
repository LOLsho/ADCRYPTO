        li(class="rare__item")
          div(class="rare__svg-wrap")
            img(class="rare__pic rare__pic3" src="./images/svg/icon-loan-2.svg" alt="percent")
          p(class="rare__desc") Любое гео<br>Все виды трафика
        li(class="rare__item")
          div(class="rare__svg-wrap")
            img(class="rare__pic rare__pic4" src="./images/svg/icon-2.svg" alt="percent")
